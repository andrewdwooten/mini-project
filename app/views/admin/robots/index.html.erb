<h1>Admin Robots Dashboard</h1>

<%= button_to 'Restock Robots', new_admin_robot_path, method:'get' %>

<ul>
<% @robots.each do |robot| %>
  <li>
    <%= robot.name %> </br>
    <%= robot.location %></br>
    <%= robot.serial_no %> </br>
    <%= button_to 'Update Robot', edit_admin_robot_path(robot), method: 'get' %>
    <%= button_to 'Delete Robot', admin_robot_path(robot), method: 'delete' %>
  </li>
<% end %>
</ul>
